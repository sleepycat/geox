{
  "name": "geox",
  "version": "1.0.3",
  "description": "Geocoding meets GraphQL",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "keywords": [
    "geocoding",
    "graphql"
  ],
  "author": "Mike Williamson <mike@korora.ca>",
  "license": "MIT",
  "dependencies": {
    "es6-promise": "^4.0.5",
    "graphql-anywhere": "^0.1.14",
    "graphql-tag": "^0.1.14",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "fetch-mock": "^5.5.0",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sleepycat/geox.git"
  },
  "bugs": {
    "url": "https://github.com/sleepycat/geox/issues"
  },
  "homepage": "https://github.com/sleepycat/geox",
  "files": [
    "dist",
    "src"
  ],
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "build": "babel --copy-files --out-dir dist/ src/",
    "test": "mocha --compilers js:babel-core/register",
    "prebuild": "node_modules/.bin/eslint src/",
    "lint": "node_modules/.bin/eslint src/"
  },
  "peerDependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
